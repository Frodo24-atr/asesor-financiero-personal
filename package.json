{
  "name": "asesor-financiero-personal",
  "version": "1.0.0",
  "description": "Asesor Financiero Personal - Herramienta inteligente para gestión financiera con análisis de ingresos, gastos y recomendaciones automáticas",
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "npm run build && npm run preview",
    "lint": "eslint . --ext ts,js --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run format:check && npm run lint",
    "start": "npm run dev"
  },
  "keywords": [
    "finanzas",
    "asesor-financiero",
    "presupuesto",
    "ingresos",
    "gastos",
    "ratios-endeudamiento",
    "salud-financiera",
    "recomendaciones",
    "dashboard",
    "pdf",
    "typescript",
    "vite"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.0",
    "typescript": "^5.6.0",
    "vite": "^7.0.0",
    "sass": "^1.80.0"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "jspdf": "^3.0.1",
    "@emailjs/browser": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuusuario/asesor-financiero-personal.git"
  },
  "bugs": {
    "url": "https://github.com/tuusuario/asesor-financiero-personal/issues"
  },
  "homepage": "https://github.com/tuusuario/asesor-financiero-personal#readme"
}
